      GEOMETRIE : MANDATORY SURFACE WITH (STRAIGHTS) VERTEX PAG.LUREF WITHOUT OVERLAPS>0.001;
    END CONST_A_CONS_POLY;

    /** couloirs et espaces réservés (art. 32) avec une largeur continue
     */
    CLASS COULOIRS_ET_ESP_RES =
      /** Type de couloir
