      /** géométrie du couloir
       */
      GEOMETRIE : MANDATORY SURFACE WITH (STRAIGHTS) VERTEX PAG.LUREF WITHOUT OVERLAPS>0.001;
      /** subdivision de la zone définie dans catégorie
       */
      GENRE : TEXT*10;
    END COULOIRS_ET_ESP_RES_PG;

